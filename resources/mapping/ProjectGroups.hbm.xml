<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.huawei.xdu.cm.model">
    <class name="ProjectGroups" table="T_BIG_VERSION">
        <meta attribute="sync-DAO">true</meta>
        <id	name="ID"  column="ID" type="int" length="0" ></id>
        <property name="NAME" column="NAME" type="string" not-null="false" length="100"/>
        <property name="OWNER" column="OWNER" type="string" not-null="false" length="50"/>
    </class>
</hibernate-mapping>